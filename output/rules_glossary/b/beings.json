        {
          "type": "a",
          "items": [
            {
              "type": "text",
              "text": "path card"
            }
          ],
          "link": {
            "id": "rules_glossary/p/path_cards",
            "anchor": "",
            "url": "/docs/rules_glossary/P/path_cards"
          }
        },
        {
          "type": "text",
          "text": "image on the back side."
        }
      ]
    },
