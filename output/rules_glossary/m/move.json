  "lookup_id": "move",
  "title": "Move",
  "content": [
    {
      "type": "p",
      "items": [
        {
          "type": "text",
          "text": "The card is moved to a different"
        },
        {
          "type": "a",
          "items": [
            {
              "type": "text",
              "text": "area"
            }
          ],
          "link": {
            "id": "rules_glossary/a/area",
            "anchor": "",
            "url": "/docs/rules_glossary/A/area"
          }
        },
        {
          "type": "text",
          "text": "than the one in which it currently sits. If an area isn\u2019t specified, you can choose to move it either"
        },
        {
          "type": "a",
          "items": [
            {
              "type": "text",
              "text": "along the way"
            }
          ],
          "link": {
            "id": "rules_glossary/a/along_the_way",
            "anchor": "",
            "url": "/docs/rules_glossary/A/along_the_way"
          }
        },
        {
          "type": "text",
          "text": "or"
        },
        {
          "type": "a",
          "items": [
            {
              "type": "text",
              "text": "within reach"
            }
          ],
          "link": {
            "id": "rules_glossary/w/within_reach",
            "anchor": "",
            "url": "/docs/rules_glossary/W/within_reach"
          }
        },
        {
          "type": "text",
          "text": "of a Ranger of your choice."
        }
      ]
    },
    {
      "type": "p",
      "items": [
        {
          "type": "text",
          "text": "If a card moves to another"
        },
        {
          "type": "a",
          "items": [
            {
              "type": "text",
              "text": "area"
            }
          ],
          "link": {
            "id": "rules_glossary/a/area",
            "anchor": "",
            "url": "/docs/rules_glossary/A/area"
          }
        },
        {
          "type": "text",
          "text": ", any cards"
        },
        {
          "type": "a",
          "items": [
            {
              "type": "text",
              "text": "attached"
            }
          ],
          "link": {
            "id": "rules_glossary/a/attach",
            "anchor": "",
            "url": "/docs/rules_glossary/A/attach"
          }
        },
        {
          "type": "text",
          "text": "to it will remain"
        },
        {
          "type": "a",
          "items": [
            {
              "type": "text",
              "text": "attached"
            }
          ],
          "link": {
            "id": "rules_glossary/a/attach",
            "anchor": "",
            "url": "/docs/rules_glossary/A/attach"
          }
        },
        {
          "type": "text",
          "text": "to it in the new"
        },
        {
          "type": "a",
          "items": [
            {
              "type": "text",
              "text": "area"
            }
          ],
          "link": {
            "id": "rules_glossary/a/area",
            "anchor": "",
            "url": "/docs/rules_glossary/A/area"
          }
        },
        {
          "type": "text",
          "text": ". If an effect would move a card that is"
        },
        {
          "type": "a",
          "items": [
            {
              "type": "text",
              "text": "attached"
            }
          ],
          "link": {
            "id": "rules_glossary/a/attach",
            "anchor": "",
            "url": "/docs/rules_glossary/A/attach"
          }
        },
        {
          "type": "text",
          "text": "to another card (without moving that other card), the card remains"
        },
        {
          "type": "a",
          "items": [
            {
              "type": "text",
              "text": "attached"
            }
          ],
          "link": {
            "id": "rules_glossary/a/attach",
            "anchor": "",
            "url": "/docs/rules_glossary/A/attach"
          }
        },
        {
          "type": "text",
          "text": "and does not move."
        }
      ]
    }
  ],
  "items": [],
  "url": "/docs/rules_glossary/M/move"
}