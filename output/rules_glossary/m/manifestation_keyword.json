{
  "id": "rules_glossary/m/manifestation_keyword",
  "lookup_group": "rules_glossary",
  "lookup_id": "manifestation_keyword",
  "title": "Manifestation (Keyword)",
  "content": [
    {
      "type": "p",
      "items": [
        {
          "type": "text",
          "text": "To play a card with the manifestation"
        },
        {
          "type": "a",
          "items": [
            {
              "type": "text",
              "text": "keyword"
            }
          ],
          "link": {
            "id": "rules_glossary/k/keywords",
            "anchor": "",
            "url": "/docs/rules_glossary/K/keywords"
          }
        },
        {
          "type": "text",
          "text": ", use ("
        },
        {
          "type": "a",
          "items": [
            {
              "type": "text",
              "text": "discard"
            }
          ],
          "link": {
            "id": "rules_glossary/d/discard",
            "anchor": "",
            "url": "/docs/rules_glossary/D/discard"
          }
        },
        {
          "type": "text",
          "text": ") a token from one of your"
        },
        {
          "type": "a",
          "items": [
            {
              "type": "text",
              "text": "equipped"
            }
          ],
          "link": {
            "id": "rules_glossary/e/equipped",
            "anchor": "",
            "url": "/docs/rules_glossary/E/equipped"
          }
        },
        {
          "type": "text",
          "text": "cards that has the"
        },
        {
          "type": "a",
          "items": [
            {
              "type": "text",
              "text": "conduit"
            }
          ],
          "link": {
            "id": "rules_glossary/c/conduit_keyword",
            "anchor": "",
            "url": "/docs/rules_glossary/C/conduit"
          }
        },
        {
          "type": "text",
          "text": "keyword."
        }
      ]
    },
    {
      "type": "h2",
      "items": [
        {
          "type": "text",
          "text": "Manifestation on Attributes"
        },
        {
          "type": "a",
          "items": [],
          "link": {
            "id": null,
            "anchor": "manifestation-on-attributes",
            "url": "#manifestation-on-attributes"
          }
        }
      ],
      "anchor": "manifestation-on-attributes"
    },
    {
      "type": "p",
      "items": [
        {
          "type": "text",
          "text": "When the Manifestation keyword appears on an"
        },
        {
          "type": "a",
          "items": [
            {
              "type": "text",
              "text": "attribute"
            }
          ],
          "link": {
            "id": "rules_glossary/a/attributes",
            "anchor": "",
            "url": "/docs/rules_glossary/A/attributes"
          }
        },
        {
          "type": "text",
          "text": "card, you must"
        },
        {
          "type": "a",
          "items": [
            {
              "type": "text",
              "text": "discard"
            }
          ],
          "link": {
            "id": "rules_glossary/d/discard",
            "anchor": "",
            "url": "/docs/rules_glossary/D/discard"
          }
        },
        {
          "type": "text",
          "text": "a token from one of your"
        },
        {
          "type": "a",
          "items": [
            {
              "type": "text",
              "text": "equipped"
            }
          ],
          "link": {
            "id": "rules_glossary/e/equipped",
            "anchor": "",
            "url": "/docs/rules_glossary/E/equipped"
          }
        },
        {
          "type": "text",
          "text": "cards with the"
        },
        {
          "type": "a",
          "items": [
            {
              "type": "text",
              "text": "conduit"
            }
          ],
          "link": {
            "id": "rules_glossary/c/conduit_keyword",
            "anchor": "",
            "url": "/docs/rules_glossary/C/conduit"
          }
        },
        {
          "type": "text",
          "text": "keyword to commit that attribute to a test."
        },
        {
          "type": "strong",
          "items": [
            {
              "type": "text",
              "text": "(Note: This is not true of non-attribute cards with the manifestation keyword. Those may be committed to tests as normal without spending a token from a conduit.)"
            }
          ]
        }
      ]
    }
  ],
  "items": [],
